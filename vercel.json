{
  "headers": [
    {
      "source": "/models/(.*)\\.usdz",
      "headers": [
        { "key": "Content-Type", "value": "model/vnd.usdz+zip" },
        { "key": "Content-Disposition", "value": "inline" }
      ]
    },
    {
      "source": "/models/(.*)\\.glb",
      "headers": [
        { "key": "Content-Type", "value": "model/gltf-binary" }
      ]
    }
  ]
}
